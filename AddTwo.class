public class AddTwo{

    public int[] addTwo(int[] numbers, int target) {
        int left = 0;
        int right = numbers.length - 1;
        
        while (left < right) {
            int sum = numbers[left] + numbers[right];
            if (sum == target) {
                return new int[]{left + 1, right + 1}; // Indices are 1-indexed
            } else if (sum < target) {
                left++;
            } else {
                right--;
            }
        }
        
        // No solution found
        return new int[]{-1, -1};
    }

    public static void main(String[] args) {
        AddTwo solution = new AddTwo();
        int[] numbers = {2, 10, 11, 15};
        int target = 25;
        int[] result = solution.addTwo(numbers, target);
        System.out.println("Output =[" + result[0] + ", " + result[1] + "] ");
    }
}